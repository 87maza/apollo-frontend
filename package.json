{
  "name": "apollo-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.4.3",
    "cross-fetch": "^3.0.4",
    "formik": "^1.5.7",
    "formik-semantic-ui": "^0.9.2",
    "graphql": "^14.4.0",
    "history": "^4.9.0",
    "node-sass": "^7.0.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-toastify": "^5.3.1",
    "recompose": "^0.30.0",
    "semantic-ui": "^2.4.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.2",
    "typescript": "3.5.2",
    "yup": "^0.26.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "semantic:build": "gulp build --cwd semantic",
    "semantic:watch": "gulp watch --cwd semantic",
    "lint:code": "eslint 'src/**/*.{ts,js,tsx,jsx}'",
    "lint:style": "stylelint 'src/**/*.{css,scss}'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "CI=true npm test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "npm run lint:code"
    ],
    "src/**/*.{css,scss}": [
      "npm run lint:style"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@types/enzyme": "^3.9.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/recompose": "^0.30.6",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-links": "^3.3.5",
    "@types/storybook__react": "^4.0.2",
    "@types/yup": "^0.26.13",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^4.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-stylelint": "^11.0.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-typescript": "^0.14.0",
    "gulp": "^4.0.2",
    "gulp-help": "^1.6.1",
    "husky": "^2.7.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "lint-staged": "^8.2.1",
    "mz": "^2.7.0",
    "plop": "^2.4.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^1.11.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript-eslint-parser": "^22.0.0"
  }
}
